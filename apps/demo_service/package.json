    "cookie-parser": "~1.4.6",
    "csv-string": "^4.1.1",
    "date-fns": "^2.29.3",
    "debug": "~2.6.9",
    "dotenv-flow": "^3.2.0",
    "express": "~4.16.4",
    "express-async-handler": "^1.2.0",
    "handlebars": "^4.7.7",
    "log4js": "^6.7.1",
    "minio": "^7.0.32",
    "module-alias": "^2.2.2",
