{
  "name": "episync-demo-service",
  "description": "Demonstration service for the EpiSync concept",
  "repository": "https://github.com/CDCgov/EpiSync.git",
  "author": "rhawes@cdc.gov",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "export NODE_OPTIONS='--trace-deprecation --abort-on-uncaught-exception' && export NODE_ENV=dev && nodemon --exec ts-node ./src/index.ts",
